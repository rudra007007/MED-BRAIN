# MED-BRAIN Backend Environment Configuration
# Copy this file to .env and fill in the values

# ============================================
# APPLICATION
# ============================================
NODE_ENV=development
PORT=3000
API_PREFIX=/api

# ============================================
# DATABASE (Supabase PostgreSQL)
# ============================================
DATABASE_URL="postgresql://postgres:password@db.supabase_project_id.supabase.co:5432/postgres?schema=public"

# ============================================
# AI SERVICE (Python FastAPI)
# ============================================
AI_SERVICE_URL=http://localhost:8001
AI_SERVICE_TIMEOUT=30000

# ============================================
# LOGGING & MONITORING
# ============================================
LOG_LEVEL=debug
